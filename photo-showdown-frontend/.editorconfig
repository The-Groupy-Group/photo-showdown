# Editor configuration, see https://editorconfig.org
root = true

[*]
indent_style = tab
indent_size = 4
end_of_line = auto
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.ts]
quote_type = double
max_line_length = 280

[*.html]
max_line_length = 280

[*.md]
max_line_length = off
trim_trailing_whitespace = false
